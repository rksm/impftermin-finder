{
  "name": "impftermin-brandenburg",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "check": "npm run typecheck && npm run lint",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "run": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts"
  },
  "devDependencies": {
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "puppeteer": "^9.0.0"
  }
}
